    "def swapalternate(arr):\n",
    "    n=len(arr)\n",
    "    for i in range(0,n-1,2):\n",
    "         arr[i],arr[i+1]=arr[i+1],arr[i]\n",
    "        \n",
    "t=int(input())\n",
    "while t>0:\n",
    "    n=int(input())\n",
    "    arr=[int(x) for x in input().split()]\n",
    "    swapalternate(arr)\n",
    "    print(*arr)\n",
    "    t-=1"
   ]
  }
