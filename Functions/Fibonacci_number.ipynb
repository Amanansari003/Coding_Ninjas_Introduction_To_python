    "def feb(num):\n",
    "    f1 = 1 \n",
    "    f2 = 1 \n",
    "    while f2 <= num :\n",
    "        if f2 == num :\n",
    "            return True \n",
    "        temp = f1+f2\n",
    "        f1 = f2 \n",
    "        f2 = temp \n",
    "    else :\n",
    "        return False\n",
    "\n",
    "\n",
    "n=int(input())\n",
    "if(feb(n)):\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
  }
