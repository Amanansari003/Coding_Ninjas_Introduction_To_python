   "source": [
    "n= int( input())\n",
    "\n",
    "n1= (n+1)//2\n",
    "n2 = n-n1\n",
    "\n",
    "for i in range (1,n1+1):\n",
    "    print(' '*(n1-i), end = '')\n",
    "    print('*'*(2*i-1), end= '')\n",
    "    print()\n",
    "\n",
    "j = n2 \n",
    "p = 1 \n",
    "while j >= 1 :\n",
    "    print(' '*p , end = '')\n",
    "    print('*'*(2*j-1), end ='')\n",
    "    print()\n",
    "\n",
    "    p+=1\n",
    "    j-=1"
   ]
  }
