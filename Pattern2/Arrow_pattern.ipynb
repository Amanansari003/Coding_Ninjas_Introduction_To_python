   "source": [
    "N= int(input())\n",
    "n1 = (N+1)//2\n",
    "n2 = N-n1\n",
    "\n",
    "i = 1 \n",
    "while i <= n1 :\n",
    "    print(\" \"*(i-1), end= '')\n",
    "    print('* '*i, end = ' ')\n",
    "    print(' ')\n",
    "    i+=1\n",
    "j = 1\n",
    "while j <= n2:\n",
    "    print(' '*(n2-j), end = '')\n",
    "    print('* '*(n2-j+1),end ='')\n",
    "    print()\n",
    "    j+=1"
   ]
  }
