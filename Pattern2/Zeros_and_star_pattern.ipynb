    "N = int(input())\n",
    "i = 1\n",
    "while i<N+1:\n",
    "    j = 1\n",
    "    while j<2*N+2:\n",
    "        if(i==j) or j==N+1 or j ==2*N+2-i:\n",
    "            print('*',end='')\n",
    "        else:\n",
    "            print('0',end='')\n",
    "        j+=1\n",
    "    i+=1\n",
    "    print()"
   ]
