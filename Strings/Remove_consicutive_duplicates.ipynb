    "\n",
    "def removeConsecutiveDuplicates(string) :\n",
    "    s = ''\n",
    "    for i in string:\n",
    "        if s == '' or i != s[len(s)-1]:\n",
    "            s+=i\n",
    "    print(s)\n",
    "\n",
    "\n",
    "#main\n",
    "string = input()\n",
    "removeConsecutiveDuplicates(string)"
   ]
